<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">
    <entity class="model.Attribute">
        <table name="t_attribute"></table>
        <attributes>
            <id name="id"></id>
            <basic name="name"></basic>
            <basic name="type"></basic>
            <basic name="scope">
                <enumerated>STRING</enumerated>
            </basic>
            <basic name="defaultValue">
                <column name="`Default`"></column>
            </basic>
            <many-to-one name="object">
                <join-column name="object_id"></join-column>
            </many-to-one>
            <one-to-many name="taggedValues" mapped-by="attribute"></one-to-many>
        </attributes>
    </entity>
</entity-mappings>
